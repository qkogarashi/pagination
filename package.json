{
  "name": "@qkogarashi/pagination",
  "version": "1.2.1",
  "description": "A pagination module for Discord.js v14 and DiscordX",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "build:typedoc": "npx typedoc src/index.ts --out ../../docs/static/api/pagination"
  },
  "contributors": [
    "Vijay Meena <vijayymmeena@gmail.com> (https://github.com/vijayymmeena)",
    "qKogarashi"
  ],
  "keywords": [
    "pagination",
    "discord.js",
    "discordjs-pagination",
    "discordjs-pagination-module",
    "embeds",
    "configurable",
    "modulable",
    "discord.js v14",
    "bot",
    "discord",
    "discordx",
    "javascript",
    "typescript",
    "utilities"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE.txt",
    "README.md",
    "SECURITY.md",
    "package.json"
  ],
  "author": "qKogarashi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qkogarashi/pagination.git"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "@types/lodash": "^4.17.17"
  },
  "dependencies": {
    "discord.js": "^14.19.3",
    "lodash": "^4.17.21"
  },
  "publishConfig": {
    "access": "public"
  }
}
